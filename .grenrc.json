{
  "dataSource": "prs",
  "prefix": "",
  "onlyMilestones": false,
  "ignoreTagsWith": ["test", "hotfix", "deploy"],
  "groupBy": {
    "🔥 Enhancements:": ["enhancement", "feature"],
    "🐛 Bug Fixes:": ["bug"],
    "⚙️ Tech:": ["tech"],
    "📜 Documentation:": ["documentation"]
  },
  "changelogFilename": "CHANGELOG.md",
  "template": {
    "issue": "- {{name}} [{{text}}]({{url}})"
  }
}
